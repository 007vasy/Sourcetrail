		</undefined_type>
		<namespace>
			<normal>#00000000</normal>
			<hover>#00000000</hover>
		</namespace>
		<typedef>
			<normal>#ededed</normal>
			<hover>#ededed</hover>
		</typedef>
		<enum_constant>
			<normal>#7BB5D4</normal>
			<hover>#42A1D4</hover>
		</enum_constant>
		<template_parameter_type>
			<normal>#ededed</normal>
